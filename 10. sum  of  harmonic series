
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number :");
        int n=sc.nextInt();
        double sum=0;
        for(int i=1;i<=n;i++){
            sum=sum+(float)1/i;
        }
        System.out.println("sum of harmonic series of "+n +" is :"+sum);
       
    }
}
